<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:parentTag="android.widget.FrameLayout">

    <com.example.myapplication.widget.CreateInputConfigPanel
        android:id="@+id/create"
        android:layout_width="216dp"
        android:layout_height="327dp"
        android:layout_gravity="center"
        android:visibility="gone" />

    <com.example.myapplication.widget.ExitDialog
        android:id="@+id/exitDialog"
        android:layout_width="240dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:visibility="gone" />

    <com.example.myapplication.widget.ClearDialog
        android:id="@+id/clearDialog"
        android:layout_width="240dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:visibility="gone" />

    <com.example.myapplication.widget.RenameView
        android:id="@+id/renameView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone" />

    <com.example.myapplication.widget.InputConfigSettingsView
        android:id="@+id/settings"
        android:layout_width="540dp"
        android:layout_height="320dp"
        android:layout_gravity="center"
        android:visibility="gone" />

    <com.example.myapplication.widget.CloudConfigView
        android:id="@+id/cloudConfig"
        android:layout_width="540dp"
        android:layout_height="320dp"
        android:layout_gravity="center"
        android:visibility="gone" />
</merge>